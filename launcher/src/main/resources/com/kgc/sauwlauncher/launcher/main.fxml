<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.kgc.sauwlauncher.launcher.Controller" alignment="BOTTOM_CENTER">
    <children>
        <HBox fx:id="group" alignment="BOTTOM_CENTER" prefHeight="100.0" prefWidth="500.0">
            <children>
                <ComboBox fx:id="versionSelector" prefHeight="40" prefWidth="190">
                    <HBox.margin>
                        <Insets right="10.0"/>
                    </HBox.margin>
                </ComboBox>
                <Button fx:id="start" prefHeight="40" prefWidth="280"/>
            </children>
        </HBox>
        <HBox prefHeight="40.0" prefWidth="500.0" alignment="CENTER">
            <children>
                <ProgressBar fx:id="progressBar" prefHeight="20.0" prefWidth="480.0" progress="1"/>
            </children>
        </HBox>
    </children>
</VBox>
